    "def palindrome(word):\n",
    "    if (word.lower()).strip() == (word[::-1].lower()).strip():\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "print(palindrome('Toma amoT'))\n",
    "print(palindrome('토마토'))\n",
    "print(palindrome('a'))\n",
    "print(palindrome('nanNAN'))"
