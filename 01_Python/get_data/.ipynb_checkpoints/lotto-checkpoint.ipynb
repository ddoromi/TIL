   ],
   "source": [
    "numbers = []\n",
    "for k, v in data.items():\n",
    "    if 'drwNo' in k:\n",
    "        numbers.append(v)\n",
    "#numbers.sort()\n",
    "\n",
    "print(numbers)"
   ]
  }
