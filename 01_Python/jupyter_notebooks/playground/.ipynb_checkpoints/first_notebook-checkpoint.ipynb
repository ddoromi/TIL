    "num1 = 3\n",
    "num2 = 7\n",
    "\n",
    "if num1 < num2 :\n",
    "    temp = num1\n",
    "    num1 = num2\n",
    "    num2 = temp\n",
    "\n",
    "#최대공약수\n",
    "a = num1\n",
    "b = num2\n",
    "\n",
    "while b > 0 :\n",
    "    c = a % b\n",
    "    a = b\n",
    "    b = c\n",
    "\n",
    "gcd = a\n",
    "\n",
    "#최소공배수\n",
    "lcm = num1 * num2 / gcd\n",
    "\n",
    "print (gcd, lcm) "
   ]
